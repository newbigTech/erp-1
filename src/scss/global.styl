  width: 16px;
  height: 16px;
  &:hover {
    background-color: rgba(7, 17, 27, .3);
    border-radius: 3px;
  }
  &:active {
    background-color: rgba(7, 17, 27, .5);
    border-radius: 3px;
  }
}

.invoice-up {
  background: url('../assets/turnUp .png') no-repeat 5px 5px;
}

.invoice-down {
  background: url('../assets/turnDown.png') no-repeat 5px 5px;
}

.invoice-turn {
  background: url('../assets/turn.png') no-repeat 5px 5px;
}

.c-tree {
  background: #fff;
  box-shadow: 1px 2px 4px 0px rgba(0, 0, 0, .12), 0px 0px 6px 0px rgba(0, 0, 0, .04);
  border-radius: 3px;
}

.goods-part {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content space-between;
  height: 32px;
  background-color: #00A3B7;
  border-radius: 3px;
  padding: 0 5px;
  box-sizing: border-box;
  & .simulate {
    display: block;
    cursor: pointer;
  }
  .classify_logo {
    display: flex;
    flex-direction: row;
    align-items: center;
    i {
      width: 25px;
      height: 25px;
      margin-right: 3px;
      background: url('../assets/all-lei.png') no-repeat center center;
    }
    .simulate-white {
      display: block;
      cursor: pointer;
      color: #fff;
    }
  }
  .butoon-left {
    width: 26px;
    height: 26px;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    border-radius: 3px;
    & .arrow-btn {
      box-sizing: border-box;
      width: 12px;
      height: 12px;
      cursor: pointer;
      background: url('../assets/white-left.png') no-repeat center center;
    }
    &:hover {
      background-color: rgba(0, 0, 0, .2);
    }
  }
}

.butoon-down {
  margin-right: 8px;
  float: left;
  width: 24px;
  height: 24px;
  background-color: #00A3B7;
  margin: 5px 8px 5px 0;
  border-radius: 3px;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  .white-down {
    width: 12px;
    height: 12px;
    background: url('../assets/white-down.png') no-repeat center center;
  }
  &:hover {
    background-color: #008A8A;
  }
}

